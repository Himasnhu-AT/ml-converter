{
  "name": "ml-converter",
  "version": "1.0.0",
  "description": "v1 of ML converter. take JSON and parse ML model based on it",
  "main": "dist/index.js",
  "module": "commonjs",
  "scripts": {
    "dev": "node dist/index.js",
    "build": "tsc",
    "test": "jest"
  },
  "author": "himasnhu-at <git>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": ".",
    "testEnvironment": "node"
  }
}
